{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run evaluation",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "blip2-opt-2.7b",
                "--pretrained",
                "Salesforce",
                "--output",
                "./output",
                "--coco_image_root",
                "./sugar-crepe/data/coco/images/val2017/",
                "--data_root",
                "./sugar-crepe/data"
            ],
            "justMyCode": false
        },
        {
            "name": "Launch Seed-Bench",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/SEED-Bench/eval.py",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "instruct_blip",
                "--anno_path",
                "${workspaceFolder}/SEED-Bench/SEED_filtered_debug.json",
                "--output-dir",
                "results"
            ],
        }
    ]
}